@model MyCvProject.Models.Entity.myExperiences
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Deneyim Ekle</h2>
<br />

<form method="post">
    <label>Başlık</label>
    <input type="text" name="title" class="form-control" required="" />

    <br />

    <label>Açıklama</label>
    <input type="text" name="subtitle" class="form-control" required="" />

    <br />

    <label>İş Tanımı</label>
    <textarea name="description" class="form-control" required="" rows="5"> </textarea> 

    <br />

    <label>Tarih</label>
    <input type="text" name="date" class="form-control" required="" placeholder="Ağustos 2021... gibi giriniz" />

    <br />

    <button class="btn btn-info">Yeni Deneyim Ekle</button>
</form>


